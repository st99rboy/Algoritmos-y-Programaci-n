// Algoritmo que evalua una expresion matematica (A+7*C)/(B+2-A)+2*B
Algoritmo SEC10
	
	Definir A, B, C Como Real;
	
	Escribir "Introduzca A: ";
	Leer A;
	Escribir "Introduzca B: ";
	Leer B;
	Escribir "Introduzca C: ";
	Leer C;
	
	Escribir "El resultado de la expresion matematica (A+7*C)/(B+2-A)+2*B es: ", (A+7*C)/(B+2-A)+2*B;
	
FinAlgoritmo
